    <form action="/signup" method="POST">
      
      
			<!--EJS FUNCTION ERROR MESSAGE-->
			<% if(message.length > 0) { %>
				<div class="alert alert-danger"><%= message %></div> 
			   <% } %>
				<div class="form-row">
					<div class="col form-group">
					  	<input type="text" name="firstName" class="form-control" placeholder="first name">
					</div> <!-- form-group end.// -->
					<div class="col form-group">
